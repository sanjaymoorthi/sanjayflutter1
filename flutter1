import 'package:flutter/material.dart';

void main() {
  runApp(const MaterialApp(
    home: MyApp(),
  ));
}

String maining ="data:image/jpeg;base64,";
                   
class MyApp extends StatefulWidget {
  const MyApp({Key? key}) : super(key: key);

  @override
  State<MyApp> createState() => _MyAppState();
}

class _MyAppState extends State<MyApp> {
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      
      body: Column(children: [
        Center(
            child: Image.network("data:image/png;base64,",
          height: 100,
          width: 600,
        )),
        SizedBox(height: 10),
        SingleChildScrollView(
          scrollDirection: Axis.horizontal,

          child: Row(children: [
            SizedBox(width: 10),
            newMethod_menu("Dindigul Biryani"),
            SizedBox(width: 10),
          newMethod_menu("Ambur Biryan"),
          SizedBox(width: 10),
          newMethod_menu("Thalassery Biryani"),
          ],),
        ),
        Container(
          height: 500,
          width: double.infinity,
          color: Colors.red,
          child: Row(
            children: [
              //left side
              Container(
                height: 500,
                width: 160,
               // color: Colors.black,
                child:SingleChildScrollView(
                child: Column(
                  children: [
                    SizedBox(height:15),
                    newMethod_left("data:image/jpeg;base64,"
                        ),
                    SizedBox(height:15),
                    newMethod_left("data:image/jpeg;base64,"),
                    SizedBox(height:15),
                    newMethod_left("data:image/jpeg;base64,"),
                    SizedBox(height:15),
                    newMethod_left("data:image/png;base64,"),
                  ],
                ),
                ),
              ),
              

              //right side
              Container(
                height: 200,
                width: 200,
                decoration: BoxDecoration(
                        color: Colors.white,
                        image: DecorationImage(image: NetworkImage(
                          maining
                        ),
                          fit: BoxFit.cover),
                        borderRadius: BorderRadius.circular(15),
                        boxShadow: [
                          BoxShadow(
                            color: Colors.black,
                            offset: Offset(4, 5),
                            blurRadius: 6,
                          )
                        ],
                      ),
              
              ),
              //
              Text("Ingredients",style:TextStyle(
                fontSize:14,
                fontWeight:FontWeight.bold,
                color:Colors.black,),
              )
            ],
          ),  
          ),
      
      ],),
    );
  }

  GestureDetector newMethod_left(biriyaniname) {
    return GestureDetector(
                    onTap: () {
                      print("Type 1");
                      setState(() {
                        maining=biriyaniname;
                      });
                    },
                    child: Container(
                      height: 100,
                      width: 100,
                      decoration: BoxDecoration(
                        color: Colors.white,
                        image: DecorationImage(image: NetworkImage(
                          biriyaniname),
                        fit: BoxFit.cover),
                        borderRadius: BorderRadius.circular(15),
                        boxShadow: [
                          BoxShadow(
                            color: Colors.black,
                            offset: Offset(4, 5),
                            blurRadius: 6,
                          )
                        ],
                      ),
                    ),
                  );
  }

  Container newMethod_menu(menuname) {
    return Container(
        height: 50,
        width: 150,
        color:Colors.black,
        child: Text(menuname,
        style: TextStyle(
                    fontSize: 17,
                    fontWeight: FontWeight.bold,
                    color: Colors.white,
                    )),
                  );
  }
}
